class_name: AcceptanceTester
modules:
    enabled:
        - WebDriver:
            url: http://127.0.0.1:8080/
            browser: firefox
            window_size: 1024x768
        - Yii2:
            part: [orm, fixtures]
            entryScript: index-test.php
            cleanup: false
            configFile: 'tests/_config/acceptance.php'
